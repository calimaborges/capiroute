{
  "name": "capiroute",
  "version": "0.2.0",
  "description": "simple routing library",
  "main": "build/index.js",
  "scripts": {
    "start": "NODE_ENV=development babel-watch src/index.js",
    "build": "NODE_ENV=production babel src -d build",
    "test": "NODE_ENV=test jest --runInBand --coverage",
    "test:watch": "NODE_ENV=test jest --runInBand --coverage --watch",
    "release": "np --no-publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calimaborges/capiroute.git"
  },
  "keywords": [
    "route",
    "routing"
  ],
  "author": "Carlos Augusto Borges (http://carlosborg.es/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/calimaborges/capiroute/issues"
  },
  "homepage": "https://github.com/calimaborges/capiroute#readme",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "babel-watch": "^2.0.6",
    "jest": "^19.0.2",
    "np": "^2.13.1"
  },
  "dependencies": {
    "normalize-path": "latest",
    "qs": "latest",
    "react": "^15.4.2"
  }
}
