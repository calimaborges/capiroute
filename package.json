{
  "name": "capiroute",
  "version": "0.4.9",
  "description": "simple routing library",
  "main": "build/index.js",
  "scripts": {
    "start": "NODE_ENV=development watch 'npm run build' src",
    "build": "NODE_ENV=production babel src -d build",
    "test": "NODE_ENV=test jest --runInBand --coverage",
    "test:watch": "NODE_ENV=test jest --runInBand --watch",
    "release": "np",
    "prepublish": "npm run build",
    "repl": "babel-node",
    "lint": "lint-staged",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calimaborges/capiroute.git"
  },
  "keywords": [
    "route",
    "routing"
  ],
  "author": "Carlos Augusto Borges (http://carlosborg.es/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/calimaborges/capiroute/issues"
  },
  "homepage": "https://github.com/calimaborges/capiroute#readme",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^19.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-jsx": "^6.23.0",
    "babel-preset-env": "^1.3.2",
    "coveralls": "^2.13.0",
    "enzyme": "^2.8.0",
    "eslint": "^3.19.0",
    "eslint-config-fbjs": "^1.1.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-jasmine": "^2.2.0",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-react": "^6.10.3",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "lint-staged": "^3.4.0",
    "np": "^2.13.1",
    "prettier": "^1.2.2",
    "prop-types": "^15.5.8",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-test-renderer": "^15.4.2",
    "watch": "^1.0.2"
  },
  "peerDependencies": {
    "react": "^15.0.0-0 || ^16.0.0-0"
  },
  "dependencies": {
    "normalize-path": "latest",
    "qs": "latest"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "git add"
    ]
  }
}
